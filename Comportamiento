// C++ code

void setup()
{
  pinMode(7, OUTPUT); //Led Verde
  pinMode(8, OUTPUT); //Led Amarillaa
  pinMode(9, OUTPUT); //Led Roja
}

void loop()
{
  digitalWrite(9, HIGH); // Encender Led Roja
  delay(5000); // Esperar durante 5 segundos
  digitalWrite(9, LOW); // Apagar Led Roja
  delay(1000); // Esperar durante 1 segundo
  digitalWrite(9, HIGH);
  delay(1000); // Esperar durante 1 segundo
  digitalWrite(9, LOW);
  delay(1000); // Esperar durante 1 segundo
  digitalWrite(8, HIGH);// Encender Led Amarilla
  delay(5000); // Esperar durante 5 segundos
  digitalWrite(8, LOW);// Apagar Led Amarilla
  delay(1000); // Esperar durante 1 segundo
  digitalWrite(8, HIGH);
  delay(1000); // Esperar durante 1 segundo
  digitalWrite(8, LOW);
  delay(1000); // Esperar durante 1 segundo
  digitalWrite(7, HIGH);// Encender Led Verde
  delay(5000); // Esperar durante 5 segundos
  digitalWrite(7, LOW);
  delay(1000); // Esperar durante 1 segundo
  digitalWrite(7, HIGH);
  delay(1000); // Esperar durante 1 segundo
  digitalWrite(7, LOW);
  delay(1000); // Esperar durante 1 segundo
  digitalWrite(8, HIGH);// Encender Led Roja
  delay(5000); // Esperar durante 5 segundos
  digitalWrite(8, LOW);// Apagar Led Amarilla
  delay(1000); // Esperar durante 1 segundos
  digitalWrite(8, HIGH);
  delay(1000); // Esperar durante 1 segundo
  digitalWrite(8, LOW);
  delay(1000); // Esperar durante 1 segundo
}
